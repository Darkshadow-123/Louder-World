{
  "name": "louder-world-events",
  "version": "1.0.0",
  "description": "Sydney Events Aggregator - Scrapes, displays, and manages events from multiple sources",
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "cd backend && npm start",
    "build": "cd frontend && npm run build",
    "scrape": "cd backend && node services/scraper.js"
  },
  "keywords": ["events", "scraping", "sydney", "mern", "react", "mongodb"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
